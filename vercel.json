{
    "rewrites": [
        {
            "source": "/",
            "destination": "https://roami.webflow.io"
        },
        {
            "source": "/blog",
            "destination": "https://roami.webflow.io/blog"
        },
        {
            "source": "/blogs/(.*)",
            "destination": "https://roami.webflow.io/blogs/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
